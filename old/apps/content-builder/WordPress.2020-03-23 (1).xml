		<guid isPermaLink="false">http://mj.wordpoets.com/?post_type=cf_elements&#038;p=592878</guid>
		<description></description>
		<content:encoded><![CDATA[[loop.@content_element module.content_elements not_empty='{module.content_elements}']
    
   	[if.yes module.repeater]
      [php.str_replace p1=".1." p2=".{{aw2.get repeater.num}}." p3="{module.main}" set="module.main" /]
    [/if.yes]
    
    [if.equal lhs='{@content_element.item.name}' rhs='{module.main}']
      [module.set module.value='{@content_element.item.value}' /]
    [/if.equal]
    
[/loop.@content_element] ]]></content:encoded>
