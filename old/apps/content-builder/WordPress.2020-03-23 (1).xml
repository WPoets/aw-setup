		<description></description>
		<content:encoded><![CDATA[[templates.add m]
  <div class="col-xs-12 form-group">
  [this_collection.print-label label='{template.label}' repeater='{template.repeater}' required='{template.required}'/]
    [this_collection.parse-activity-elements content_element='{template.content_element}' repeater='{template.repeater}' required='{template.required}'/]
 	<input 	type="[template.get type default=text]"
      			class="form-control" 
      			[if.yes  template.repeater]
      			  [php.str_replace p1="." p2="_" p3="{template.main}" set="template.updated_main" /]
      			  [php.str_replace p1=".1." p2=".++." p3="{template.main}" set="template.updated_name_pattern" /]
      			  [php.str_replace p1=".1." p2="_++_" p3="{template.main}" set="template.updated_id_pattern" /]
        			id="[template.get updated_main /]"
        			name="[template.get main /]"
        			data-pattern-name="[template.get updated_name_pattern /]" 
        			data-pattern-id="[template.get updated_id_pattern /]"
        		[/if.yes]
        		[if.else]
        		  id="[template.get main /]"
        		  name="[template.get main /]"
        		[/if.else]
      			value="[template.get value /]"
      			data-activity="yes"
      			[if.not_empty template.required]
      			[/if.not_empty]
      			[if.and cond="{template.required}" not_equal="yes"]
      			[/if.and]
      			[if.else]
      			  required
      			[/if.else]
      			[if.not_equal lhs="{{aw2.get template.type}}" rhs=""]
      			  min="1"
      			[/if.not_equal]
    /> 
    <span class="help-block"></span>
  </div>
 	 	 
[/templates.add]]]></content:encoded>
