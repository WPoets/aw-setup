					<item>
		<title>option-answer</title>
		<link>http://mj.wordpoets.com/?post_type=cf_elements&#038;p=597275</link>
		<pubDate>Tue, 17 Mar 2020 05:23:25 +0000</pubDate>
		<dc:creator><![CDATA[Amit]]></dc:creator>
		<guid isPermaLink="false">http://mj.wordpoets.com/?post_type=cf_elements&#038;p=597275</guid>
		<description></description>
		<content:encoded><![CDATA[[templates.add main]
  <div class="col-xs-12 form-group">
    [this_collection.print-label /]
    [this_collection.parse-activity-elements  /]
    [php.is_array p1='{module.value}' set="module.sc.val_arr" /]
    <select name="[module.get main /]" data-activity="yes" [module.get option_multiple default="" /]>
      <option value=""></option>
      [loop.@for start=1 stop="{{aw2.get module.option_length default=6}}"]
        [if.true module.sc.val_arr]
          <option value="[aw2.get item /]" [do.@x list='{{aw2.get module.value comma=true}}' contains='{item}']selected[/do.@x]>[aw2.get item /]</option>
        [/if.true]
        [if.else]
          <option value="[aw2.get item /]" [do.@x cond='{module.value}' equal='{item}']selected[/do.@x]>[aw2.get item /]</option>
        [/if.else]
      [/loop.@for]
    </select>
    <span class="help-block"></span>
  </div>
[/templates.add]]]></content:encoded>
