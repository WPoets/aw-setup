					<item>
		<title>html-block</title>
		<link>http://mj.wordpoets.com/?post_type=cf_elements&#038;p=592874</link>
		<pubDate>Mon, 17 Feb 2020 16:57:10 +0000</pubDate>
		<dc:creator><![CDATA[Amit]]></dc:creator>
		<guid isPermaLink="false">http://mj.wordpoets.com/?post_type=cf_elements&#038;p=592874</guid>
		<description></description>
		<content:encoded><![CDATA[[templates.add main]
  <div class="col-xs-12 form-group">
    [this_collection.print-label label='{module.label}' repeater='{module.repeater}' required='{module.required}'/]
    [this_collection.parse-activity-elements content_element='{module.content_element}' repeater='{module.repeater}' required='{module.required}'/]
    <textarea class="form-control tinymce" 
        			[if.yes module.repeater]
        			  [php.str_replace p1="." p2="_" p3="{module.main}" set="module.updated_main" /]
        			  [php.str_replace p1=".1." p2=".++." p3="{module.main}" set="module.updated_name_pattern" /]
        			  [php.str_replace p1=".1." p2="_++_" p3="{module.main}" set="module.updated_id_pattern" /]
          			id="[module.get updated_main /]"
          			name="[module.get main /]"
          			data-pattern-name="[module.get updated_name_pattern /]" 
          			data-pattern-id="[module.get  updated_id_pattern /]"
          		[/if.yes]
          		[if.else]
          		  id="[module.get main /]"
          		  name="[module.get main /]"
          		[/if.else]
        			data-activity="yes"
        			[if.not_empty module.required].
        			[/if.not_empty]
        			[if.and cond="{module.required}" not_equal="yes"]
        			[/if.and]
        			[if.else]
        			  required
        			[/if.else]>[module.get value /]</textarea>
    <span class="help-block"></span>
  </div>

[/templates.add]]]></content:encoded>
